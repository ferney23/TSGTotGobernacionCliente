<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="tsg" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>com.tsg.co.model.Clases</class>
    <class>com.tsg.co.model.Entregas</class>
    <class>com.tsg.co.model.Estudiante</class>
    <class>com.tsg.co.model.InfoGrado</class>
    <class>com.tsg.co.model.MaterialEstudio</class>
    <class>com.tsg.co.model.Materias</class>
    <class>com.tsg.co.model.Profesor</class>
    <class>com.tsg.co.model.Subida</class>
    <class>com.tsg.co.model.Tareas</class>
    <class>com.tsg.co.model.Version</class>
    <class>com.tsg.co.model.AchivosTot</class>
    <class>com.tsg.co.model.Kiosko</class>
    <class>com.tsg.co.model.Usuario</class>
    <class>com.tsg.co.model.ArchivoMensajeKiosco</class>
    <class>com.tsg.co.model.MensajeKiosco</class>
    <class>com.tsg.co.model.RespuestaMensaje</class>
    <properties>
      <property name="javax.persistence.jdbc.driver" value="org.h2.Driver"/>
      <property name="javax.persistence.jdbc.url" value="jdbc:h2:./src/main/resources/basetot"/>
      <property name="javax.persistence.jdbc.user" value=""/>
      <property name="javax.persistence.jdbc.password" value=""/>
      <property name="hibernate.dialect" value="org.hibernate.dialect.H2Dialect"/>
      <property name="hibernate.hbm2ddl.auto" value="update"/>
      <property name="show_sql" value="true"/>
      <property name="hibernate.temp.use_jdbc_metadata_defaults" value="true"/>
      <property name="connection.pool_size" value="100"/>
    </properties>
  </persistence-unit>
</persistence>
